    "date-fns": "3.6.0",
    "papaparse": "5.4.1",
    "react": "18.2.0",
    "react-bootstrap": "2.10.2",
    "react-bootstrap-icons": "1.11.3",
    "react-bootstrap-typeahead": "6.3.2",
    "react-card-flip": "1.2.2",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
