    "bootswatch": "^5.3.2",
    "date-fns": "^3.3.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.1",
    "react-bootstrap-icons": "^1.10.3",
    "react-bootstrap-typeahead": "^6.3.2",
    "react-card-flip": "^1.2.2",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "recoil": "^0.7.7",
